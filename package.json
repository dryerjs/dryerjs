{
  "name": "dryerjs",
  "version": "0.7.0",
  "description": "A platform for building GraphQL APIs with TypeScript and MongoDB in a declarative way.",
  "license": "MIT",
  "type": "commonjs",
  "author": "Brian Nguyen",
  "repository": "https://github.com/dryerjs/dryerjs.git",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "format": "prettier \"src/**/*.ts\" \"e2e/**/*.ts\" \"src/**/*.ts\" -w",
    "format:ci": "prettier \"src/**/*.ts\" \"e2e/**/*.ts\" \"src/**/*.ts\" -l",
    "lint": "eslint \"{src,lib,e2e}/**/*.ts\" --fix",
    "lint:ci": "eslint \"{src,lib,e2e}/**/*.ts\" --max-warnings=0",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "start": "nodemon --exec ts-node src/main.ts",
    "start:debug": "nodemon --exec ts-node src/main.ts --debug",
    "start:dev": "npm run start",
    "unit": "npm run test:unit",
    "test:unit": "jest --config jest-unit.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "e2e": "npm run test:e2e",
    "test:e2e": "jest --config ./jest-e2e.json --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "NODE_ENV=test npm run clean-cov && npm run test:unit -- --coverage && npm run test:e2e -- --coverage && npm run collect-cov && npm run merge-cov",
    "clean-cov": "rm -rf coverage",
    "collect-cov": "mv coverage/unit/coverage-final.json coverage/unit.json && mv coverage/e2e/coverage-final.json coverage/e2e.json",
    "merge-cov": "nyc report -t coverage --report-dir coverage --reporter=text-summary --reporter=lcov",
    "show-cov": "open coverage/lcov-report/index.html",
    "semantic-release": "semantic-release",
    "cspell": "cspell \"**\"",
    "postinstall": "husky install || echo 'husky not found'"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-angular": "17.7.0",
    "@nestjs/cli": "9.5.0",
    "@nestjs/common": "10.2.2",
    "@nestjs/core": "10.2.2",
    "@nestjs/mongoose": "10.0.1",
    "@nestjs/platform-express": "10.2.2",
    "@nestjs/schematics": "9.0.0",
    "@nestjs/testing": "10.2.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.4",
    "@types/node": "18.17.12",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cspell": "7.3.8",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "lint-staged": "14.0.1",
    "mongoose": "7.5.0",
    "nodemon": "3.0.1",
    "nyc": "15.1.0",
    "prettier": "3.0.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "semantic-release": "22.0.5",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "@apollo/server": "^4.0.0",
    "@nestjs/apollo": "^10.0.0 || ^11.0.0 || ^12.0.0",
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/graphql": "^10.0.0 || ^11.0.0 || ^12.0.0",
    "@nestjs/mongoose": "^9.0.0 || ^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dataloader": "^2.2.2"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  }
}
