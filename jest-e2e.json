{
  "moduleNameMapper": {
    "dryerjs": "<rootDir>/index.ts"
  },
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "src",
  "testEnvironment": "node",
  "testRegex": ".e2e.spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.ts"
  ],
  "coveragePathIgnorePatterns": [
    "<rootDir>/e2e/",
    "<rootDir>/dist/",
    "<rootDir>/coverage/",
    "<rootDir>/example/",
    ".spec.ts"
  ],
  "coverageDirectory": "../coverage/e2e",
  "coverageReporters": ["json", "html"]
}
