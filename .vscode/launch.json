{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Debug Attach",
        "processId": "${command:PickProcess}",
        "restart": true,
        "protocol": "inspector"
      },
      {
        "command": "npm run test ${relativeFile}",
        "name": "Test Unit",
        "request": "launch",
        "type": "node-terminal"
    },
      {
        "command": "npm run test:e2e ${relativeFile}",
        "name": "Test E2E",
        "request": "launch",
        "type": "node-terminal"
    },
    ]
}
